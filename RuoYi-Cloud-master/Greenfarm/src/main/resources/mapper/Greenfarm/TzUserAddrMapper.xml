<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.Greenfarm.mapper.TzUserAddrMapper">
    
    <resultMap type="TzUserAddr" id="TzUserAddrResult">
        <result property="addrId"    column="addr_id"    />
        <result property="userId"    column="user_id"    />
        <result property="receiver"    column="receiver"    />
        <result property="provinceId"    column="province_id"    />
        <result property="province"    column="province"    />
        <result property="city"    column="city"    />
        <result property="cityId"    column="city_id"    />
        <result property="area"    column="area"    />
        <result property="areaId"    column="area_id"    />
        <result property="postCode"    column="post_code"    />
        <result property="addr"    column="addr"    />
        <result property="mobile"    column="mobile"    />
        <result property="status"    column="status"    />
        <result property="commonAddr"    column="common_addr"    />
        <result property="createTime"    column="create_time"    />
        <result property="version"    column="version"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectTzUserAddrVo">
        select addr_id, user_id, receiver, province_id, province, city, city_id, area, area_id, post_code, addr, mobile, status, common_addr, create_time, version, update_time from tz_user_addr
    </sql>

    <select id="selectTzUserAddrList" parameterType="TzUserAddr" resultMap="TzUserAddrResult">
        <include refid="selectTzUserAddrVo"/>
        <where>  
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="receiver != null  and receiver != ''"> and receiver = #{receiver}</if>
            <if test="provinceId != null "> and province_id = #{provinceId}</if>
            <if test="province != null  and province != ''"> and province = #{province}</if>
            <if test="city != null  and city != ''"> and city = #{city}</if>
            <if test="cityId != null "> and city_id = #{cityId}</if>
            <if test="area != null  and area != ''"> and area = #{area}</if>
            <if test="areaId != null "> and area_id = #{areaId}</if>
            <if test="postCode != null  and postCode != ''"> and post_code = #{postCode}</if>
            <if test="addr != null  and addr != ''"> and addr = #{addr}</if>
            <if test="mobile != null  and mobile != ''"> and mobile = #{mobile}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="commonAddr != null "> and common_addr = #{commonAddr}</if>
            <if test="version != null "> and version = #{version}</if>
        </where>
    </select>
    
    <select id="selectTzUserAddrByAddrId" parameterType="Long" resultMap="TzUserAddrResult">
        <include refid="selectTzUserAddrVo"/>
        where addr_id = #{addrId}
    </select>
        
    <insert id="insertTzUserAddr" parameterType="TzUserAddr" useGeneratedKeys="true" keyProperty="addrId">
        insert into tz_user_addr
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null and userId != ''">user_id,</if>
            <if test="receiver != null">receiver,</if>
            <if test="provinceId != null">province_id,</if>
            <if test="province != null">province,</if>
            <if test="city != null">city,</if>
            <if test="cityId != null">city_id,</if>
            <if test="area != null">area,</if>
            <if test="areaId != null">area_id,</if>
            <if test="postCode != null">post_code,</if>
            <if test="addr != null">addr,</if>
            <if test="mobile != null">mobile,</if>
            <if test="status != null">status,</if>
            <if test="commonAddr != null">common_addr,</if>
            <if test="createTime != null">create_time,</if>
            <if test="version != null">version,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null and userId != ''">#{userId},</if>
            <if test="receiver != null">#{receiver},</if>
            <if test="provinceId != null">#{provinceId},</if>
            <if test="province != null">#{province},</if>
            <if test="city != null">#{city},</if>
            <if test="cityId != null">#{cityId},</if>
            <if test="area != null">#{area},</if>
            <if test="areaId != null">#{areaId},</if>
            <if test="postCode != null">#{postCode},</if>
            <if test="addr != null">#{addr},</if>
            <if test="mobile != null">#{mobile},</if>
            <if test="status != null">#{status},</if>
            <if test="commonAddr != null">#{commonAddr},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="version != null">#{version},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateTzUserAddr" parameterType="TzUserAddr">
        update tz_user_addr
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null and userId != ''">user_id = #{userId},</if>
            <if test="receiver != null">receiver = #{receiver},</if>
            <if test="provinceId != null">province_id = #{provinceId},</if>
            <if test="province != null">province = #{province},</if>
            <if test="city != null">city = #{city},</if>
            <if test="cityId != null">city_id = #{cityId},</if>
            <if test="area != null">area = #{area},</if>
            <if test="areaId != null">area_id = #{areaId},</if>
            <if test="postCode != null">post_code = #{postCode},</if>
            <if test="addr != null">addr = #{addr},</if>
            <if test="mobile != null">mobile = #{mobile},</if>
            <if test="status != null">status = #{status},</if>
            <if test="commonAddr != null">common_addr = #{commonAddr},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="version != null">version = #{version},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where addr_id = #{addrId}
    </update>

    <delete id="deleteTzUserAddrByAddrId" parameterType="Long">
        delete from tz_user_addr where addr_id = #{addrId}
    </delete>

    <delete id="deleteTzUserAddrByAddrIds" parameterType="String">
        delete from tz_user_addr where addr_id in 
        <foreach item="addrId" collection="array" open="(" separator="," close=")">
            #{addrId}
        </foreach>
    </delete>
</mapper>